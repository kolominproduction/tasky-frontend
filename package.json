  "scripts": {
    "dev": "webpack serve --env mode=development",
    "build:dev": "webpack --env mode=development",
    "build:prod": "webpack --env mode=production",
    "prettier": "npx prettier --write \"**/!(*config).{ts,tsx,json}\"",
    "lint:ts": "eslint",
    "lint:ts:fix": "eslint --fix",
    "lint:css": "npx stylelint",
    "lint:css:fix": "npx stylelint --fix",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:ui": "npx loki test",
    "test:ui:ok": "npx loki approve",
    "test:ui:report": "npx reg-cli ./.loki/current ./.loki/reference ./.loki/difference -R ./reports/storybook/report.html",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006 -c ./config/storybook",
    "storybook:build": "storybook build -c ./config/storybook"
  },
